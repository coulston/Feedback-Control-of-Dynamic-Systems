figure(1)
clf
set(1,'position',[10 10 600 400])
set(1,'papersize',[6 4])
set(1,'paperposition',[0 0 6 4])
omega=1;
zetalist=[.1 .5 .7 1];
stylelist={'-','--','-.',':'};
colorlist={'b','r','g','c'};
ylist=[];
i=1;
for zeta=zetalist,
 sys=tf([omega^2],[1 2*zeta*omega omega^2])
 [y,t]=step(sys,12);
plot(t,y,'linewidth',2,'color',colorlist{i},'linestyle',stylelist{i})
i=i+1;
hold on
end;
hold off
plotpdftex(1,'risetime1',[1.4 1])


